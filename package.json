    "postcss-scss": "2.0.0",
    "react-modal": "3.8.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.1",
    "react-transition-group": "4.0.1",
    "regenerator-runtime": "^0.13.7",
    "resolve-url-loader": "3.1.0",
    "sass-loader": "7.1.0",
    "st": "1.2.2",
    "style-loader": "0.23.1",
    "typescript": "4.0.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "4.19.1",
    "webpack-babel-env-deps": "1.5.0",
